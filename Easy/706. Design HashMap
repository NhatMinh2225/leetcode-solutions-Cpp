class MyHashMap {
public:
vector<int> hashTable = vector<int>(1'000'001, -1);

    MyHashMap() {
        
    }
    
    void put(int key, int value) {
        hashTable[key] = value;
    }
    
    int get(int key) {
        return hashTable[key];

    }
    
    void remove(int key) {
        hashTable[key] = -1;
    }
};

/**
 * Your MyHashMap object will be instantiated and called as such:
 * MyHashMap* obj = new MyHashMap();
 * obj->put(key,value);
 * int param_2 = obj->get(key);
 * obj->remove(key);
 */
