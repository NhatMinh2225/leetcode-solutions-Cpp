class Solution {
public:
    vector<int> majorityElement(vector<int>& nums) {
        int n = nums.size()/3;
        vector<int> majorityElement;
        unordered_map<int,int> map;

        for(int i = 0; i < nums.size(); i++){
            if (!map.count(nums[i])){
                map.insert({nums[i], 1});
            }
            else {
                map[nums[i]] ++;
            }
        }

        for(auto &x : map){
            if (x.second > (n)){
                majorityElement.push_back(x.first);
            }
        }
        return majorityElement;

    }
};
