class Solution {
public:

    string encode(vector<string>& strs) {
        string s;
        for (int i = 0; i < strs.size(); i++){
            s = s + strs[i] + "#42";
        }
        return s;
    }

    vector<string> decode(string s) {
        vector<string> result;
        int count = 0;
        for (int i = 0; i < s.size(); i++){
            if (result.size() == 0){
                string temp = "";
                result.push_back(temp); 
            }
            if (s[i] == '#' && s[i+1] == '4' && s[i+2] == '2' && ((i+3) >= 0 && (i+3) < s.size())){
                i=i+2;
                count++;
                string temp = "";
                result.push_back(temp);
                continue;
            }
            if((i+3) < s.size()){
                result.at(count) += s.at(i);
            }
        }
        return result;
    }
};
