

class Solution {
public:
    vector<vector<int>> threeSum(vector<int>& nums) {
        set <vector<int>> setting;
        vector<vector<int>> threeSum;
        sort (nums.begin(), nums.end());

        for (int i : nums){
            cout << i << " " ;
        }
        cout << endl;

        
        for (int i = 0; i < nums.size(); i++){
            int fixed = nums[i];
            int first = 0;
            int last = nums.size() - 1;
            vector<int> temp;
            
            while (first < last){
                if (first == i && first < nums.size() - 1){
                    first ++;
                }
                else if (last == i && last > 0){
                    last--;
                }
                else if (nums[first] + nums[last] < -fixed && first < nums.size() - 1){
                    first++;
                }
                else if (nums[first] + nums[last] > -fixed && last > 0){
                    last--;
                }
                else if (nums[first] + nums[last] == -fixed){
                    temp.push_back(nums[last]);
                    temp.push_back(nums[first]);
                    temp.push_back(fixed);
                    sort(temp.begin(), temp.end());
                    setting.insert(temp);
                    temp.clear();
                        first ++;

                        last--;

                }     
            }
        }
        threeSum.assign(setting.begin(), setting.end());
        return threeSum;
    }
};
