class Solution {
public:
    int firstMissingPositive(vector<int>& nums) {
        int n = nums.size();

        unordered_set <int> mp;
        for (int x : nums){
            mp.insert(x);
        }

        for (int i = 1; i < n+1; i++){
            if (!mp.count(i)){
                return i;
            }
        }

        if(mp.count(n)){
            return n+1;
        }

        return 1;
    }
};


